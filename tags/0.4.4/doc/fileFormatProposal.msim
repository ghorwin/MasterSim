[MasterSimProject]

[SimulationParameter]
	tstart          = 0.0
	tend            = 10.0
	tstepmax        = 0.01
	tstepstart      = 0.01
	MasterMode      = GAUSS_JACOBI
	MaxIterations   = 1
	it_tol_abs      = 1.0E-6
