			ResultRootDir	= Therakles_test

	[Simulator]
		Name		= ControlledTemperatureCSv1
		FMU			= fmus/fmi-standard.org/linux64/CS_v1/ControlledTemperature.fmu
		Priority	= 1
		Cycle		= 1

	[Simulator]
		Name		= ControlledTemperatureCSv2
		FMU			= fmus/fmi-standard.org/linux64/CS_v2/ControlledTemperature.fmu
		Priority	= 2
		Cycle		= 2


