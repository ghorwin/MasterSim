<?xml version="1.0" encoding="UTF-8"?>
<BlockMod>
	<!--Blocks-->
	<Blocks>
		<Block name="Prey">
			<Position>64, 16</Position>
			<Size>96, 128</Size>
			<!--Sockets-->
			<Sockets>
				<Socket name="y">
					<Position>0, 16</Position>
					<Orientation>Horizontal</Orientation>
					<Inlet>true</Inlet>
				</Socket>
				<Socket name="x">
					<Position>96, 16</Position>
					<Orientation>Horizontal</Orientation>
					<Inlet>false</Inlet>
				</Socket>
			</Sockets>
		</Block>
		<Block name="values">
			<Position>-144, 0</Position>
			<Size>96, 128</Size>
			<!--Sockets-->
			<Sockets>
				<Socket name="Prey.x">
					<Position>96, 16</Position>
					<Orientation>Horizontal</Orientation>
					<Inlet>false</Inlet>
				</Socket>
				<Socket name="Predator.y">
					<Position>96, 32</Position>
					<Orientation>Horizontal</Orientation>
					<Inlet>false</Inlet>
				</Socket>
			</Sockets>
		</Block>
	</Blocks>
	<!--Connectors-->
	<Connectors>
		<Connector name="auto-named">
			<Source>values.Predator.y</Source>
			<Target>Prey.y</Target>
			<!--Connector segments (between start and end lines)-->
			<Segments>
				<Segment>
					<Orientation>Vertical</Orientation>
					<Offset>0</Offset>
				</Segment>
				<Segment>
					<Orientation>Horizontal</Orientation>
					<Offset>48</Offset>
				</Segment>
			</Segments>
		</Connector>
	</Connectors>
</BlockMod>
